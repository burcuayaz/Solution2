@model IEnumerable<DataEntity.TBL_MUSTERILER>
@{
    ViewBag.Title = "GetMusteriList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">AdSoyad</th>
            <th scope="col">Telefon</th>
            <th scope="col">Adres</th>
            <th scope="col">Mail</th>
            <th scope="col">Kayıt Tarihi</th>
        </tr>
    </thead>
    @foreach (var k in Model)
    {
        <tbody>
            <tr>
                <td>@k.ID</td>
                <td>@k.ADSOYAD</td>
                <td>@k.TELEFON</td>
                <td>@k.ADRES</td>
                <td>@k.MAIL</td>
                <td>@k.KAYIT_TARIHI</td>
            </tr>
        </tbody>
    }
</table>